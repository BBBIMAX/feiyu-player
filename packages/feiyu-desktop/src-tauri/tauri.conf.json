{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "Feiyu",
  "version": "1.0.0",
  "identifier": "xbox.work.feiyu",
  "build": {
    "beforeDevCommand": "cd ../feiyu && pnpm dev:desktop",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../../feiyu/dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "飞鱼",
        "width": 950,
        "height": 650,
        "minWidth": 300,
        "minHeight": 600,
        "center": true,
        "hiddenTitle": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": "zh-CN"
      }
    },
    "copyright": "Copyright © 2023 Del.Wang",
    "shortDescription": "一个漂亮得不像实力派的在线视频播放器 ✨",
    "longDescription": "让每个人都能通过电影的力量，获得对生活的新体验和新认知，怀着永不熄灭的热忱，勇敢地去追求、去探索、去发现生命的意义和价值。",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "entitlements": null,
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  }
}
