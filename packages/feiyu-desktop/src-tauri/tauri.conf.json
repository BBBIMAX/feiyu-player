{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "Feiyu",
  "version": "1.0.0",
  "identifier": "xbox.work.feiyu",
  "build": {
    "beforeDevCommand": "cd ../feiyu && pnpm desktop && pnpm dev",
    "beforeBuildCommand": "cd ../feiyu && pnpm desktop && pnpm build",
    "frontendDist": "../feiyu/dist",
    "devUrl": "http://localhost:3000"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "width": 800,
        "resizable": true,
        "title": "飞鱼",
        "visible": true,
        "center": true,
        "label": "main"
      }
    ],
    "security": {
      "csp": "default-src x-http: x-https: blob: data: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": {
        "language": "zh-CN"
      }
    },
    "copyright": "GPL-3.0-only",
    "shortDescription": "一个漂亮得不像实力派的在线视频播放器 ✨",
    "longDescription": "让每个人都能通过电影的力量，获得对生活的新体验和新认知，怀着永不熄灭的热忱，勇敢地去追求、去探索、去发现生命的意义和价值。",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "entitlements": null,
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    }
  }
}
